var numero = Number( prompt("Digite o numero"));

alert("Voce digitou o numero");


    if (numero <  0) {

      
      alert(" O numero e negativo");
    }
     
    if (numero > 0){
    
        alert("O numero e possitivo");
      
      }


    if (numero == 0) {
     
      alert("o numero e zero")

    }
